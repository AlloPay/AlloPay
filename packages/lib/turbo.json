{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "contracts#build"],
      "inputs": ["src/**"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["src/**"]
    }
  }
}
