{
  "name": "lib",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch --preserveWatchOutput",
    "generate:contracts": "typechain --target ethers-v5 --out-dir ./src/contracts --input-dir '../contracts/abi' '../contracts/abi/*.json'",
    "generate:typechain": "typechain --target ethers-v5 --out-dir ./src/typechain './abi/*.json'",
    "clean": "npx rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.3",
    "@typechain/ethers-v5": "^11.1.1",
    "abitype": "^0.9.8",
    "add": "^2.0.6",
    "ethers": "^5.7.2",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.3.10",
    "neverthrow": "^6.0.0",
    "ts-pattern": "^5.0.5",
    "ts-toolbelt": "^9.6.0",
    "typechain": "^8.3.1",
    "viem": "^1.19.3",
    "yarn": "^1.22.19",
    "zksync-web3": "^0.14.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
