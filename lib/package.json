{
  "name": "lib",
  "private": true,
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start": "yarn build --watch",
    "build": "yarn generate && tsc --build",
    "generate": "yarn generate:contracts && yarn generate:typechain",
    "generate:contracts": "typechain --target ethers-v5 --out-dir ./src/contracts --input-dir '../contracts/abi' '../contracts/abi/*.json'",
    "generate:typechain": "typechain --target ethers-v5 --out-dir ./src/typechain './abi/*.json'",
    "clean": "npx rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.1",
    "@typechain/ethers-v5": "^10.2.0",
    "ethers": "^5.7.2",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.3.9",
    "typechain": "^8.1.1",
    "zksync-web3": "^0.12.5"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
