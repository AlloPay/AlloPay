{
  "name": "site",
  "private": true,
  "scripts": {
    "build": "(cd .. && yarn lib build) && yarn generate && docusaurus build",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate": "yarn generate:docs && yarn generate:gql",
    "generate:docs": "yarn docusaurus graphql-to-doc",
    "generate:gql": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@apollo/explorer": "^3.4.0",
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@docusaurus/remark-plugin-npm2yarn": "^2.4.0",
    "@emotion/react": "^11.10.6",
    "@graphql-markdown/docusaurus": "^1.17.0",
    "@mdx-js/react": "^1.6.22",
    "bufferutil": "^4.0.7",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "dotenv": "^16.1.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lib": "workspace:*",
    "plugin-image-zoom": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.7.7",
    "sass": "^1.58.1",
    "use-deep-compare-effect": "^1.8.1",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.4.0",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@tsconfig/docusaurus": "1.0.6",
    "typescript": "^5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
