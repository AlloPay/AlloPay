{
  "files.exclude": {
    // Folders excluded by all but root
    "**/.vscode": false,
    "**/dist": false,
    "**/node_modules": false,
    "**/generated": false,
    "**/__generated__": false
  },
  "search.useIgnoreFiles": true,
  "prettier.configPath": ".config/.prettierrc",
  "prettier.ignorePath": ".prettierignore",
  "jest.testExplorer": {
    "enabled": false
  },
  "jest.monitorLongRun": 180000
}
