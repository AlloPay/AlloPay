{
  "files.exclude": {
    // Folders excluded by all but root
    "**/.vscode": false,
    "**/dist": false,
    "**/node_modules": false,
    "**/generated": false,
    "**/__generated__": false
  },
  "prettier.configPath": ".config/.prettierrc",
  "prettier.ignorePath": ".prettierignore",
  "jest.testExplorer": {
    "enabled": false
  },
  "eslint.options": {
    "overrideConfigFile": ".config/.eslintrc.js",
    "ignorePath": ".config/.eslintignore"
  }
}
