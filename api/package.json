{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:libs && yarn generate && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"src/**/*.ts\" -c ../.eslintrc.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "prisma": "yarn with-env -- prisma",
    "generate": "yarn prisma generate && yarn generate:gql",
    "generate:gql": "DOTENV_CONFIG_PATH=\"../.env\" graphql-codegen --require dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@golevelup/ts-jest": "^0.3.2",
    "@graphql-tools/delegate": "^8.5.4",
    "@graphql-tools/stitch": "^8.5.2",
    "@graphql-tools/utils": "^8.6.3",
    "@graphql-tools/wrap": "^8.4.6",
    "@nestjs/apollo": "^10.0.7",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/graphql": "^10.0.7",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/terminus": "^8.0.6",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.1.0",
    "@quixo3/prisma-session-store": "^3.1.8",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "workspace:*",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "ethers": "^5.6.9",
    "express-session": "^1.17.2",
    "fetch-retry": "^5.0.3",
    "graphql": "^16.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.15.0",
    "graphql-type-json": "^0.3.2",
    "isomorphic-fetch": "^3.0.0",
    "lib": "workspace:*",
    "luxon": "^3.0.1",
    "nestjs-prisma": "^0.16.0",
    "prisma": "^4.1.0",
    "prisma-graphql-type-decimal": "^2.0.4",
    "prisma-nestjs-graphql": "^16.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "siwe": "^1.1.6",
    "type-graphql": "^1.1.1",
    "zksync-web3": "^0.7.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.8.0",
    "@graphql-codegen/typescript": "2.7.1",
    "@graphql-codegen/typescript-operations": "2.5.1",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^28.1.3",
    "@types/luxon": "^2.3.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "apollo": "2.34.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.7.4"
  }
}
