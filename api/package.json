{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --debug --watch --preserveWatchOutput",
    "start:production": "yarn db:migrate && node dist/dbschema/seed.js && yarn serve",
    "serve": "node dist/src/main.js",
    "generate:ampli": "ampli pull -p ./src/util/ampli --omit-api-keys",
    "test": "yarn with-env -- jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "tsx --inspect-brk ../node_modules/.bin/jest --runInBand",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "with-env": "npx -y dotenv-cli -e ../.env",
    "generate:schema": "yarn prebuild && SKIP_ENV_CHECK=true nest start --entryFile generate-schema",
    "generate:edgeql": "npx -y concurrently -n edgeql-js,interfaces -c blue,magenta \"npx @edgedb/generate edgeql-js --target ts --force-overwrite\" \"npx @edgedb/generate interfaces\"",
    "generate:gql": "graphql-codegen --config codegen.ts",
    "db:seed": "tsx ./dbschema/seed.ts",
    "db:migrate": "yarn with-env -- edgedb migrate"
  },
  "dependencies": {
    "@amplitude/analytics-node": "^1.3.5",
    "@apollo/server": "^4.9.5",
    "@bull-board/express": "^5.10.2",
    "@bull-board/nestjs": "^5.10.2",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/terminus": "^10.2.0",
    "@pythnetwork/pyth-evm-js": "^1.29.2",
    "@sentry/node": "^7.90.0",
    "@songkeys/nestjs-redis": "^10.0.0",
    "abitype": "^0.9.8",
    "bullmq": "^4.16.0",
    "chains": "workspace:*",
    "chardet": "^2.0.0",
    "class-transformer": "^0.5.1",
    "connect-redis": "^7.1.0",
    "cross-fetch": "^4.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "edgedb": "^1.4.1",
    "ethers": "^6.9.0",
    "expo-server-sdk": "^3.7.0",
    "express-session": "^1.17.3",
    "fetch-retry": "^5.0.6",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.22.4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.3",
    "ioredis": "^5.3.2",
    "jose": "^5.1.3",
    "lib": "workspace:*",
    "luxon": "^3.4.4",
    "nestjs-request-context": "^3.0.0",
    "neverthrow": "^6.1.0",
    "redis-semaphore": "^5.5.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "siwe": "^2.1.4",
    "ts-deepmerge": "^6.2.0",
    "ts-pattern": "^5.0.6",
    "viem": "^1.20.3"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.4.1",
    "@golevelup/ts-jest": "^0.4.0",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.3.7",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  }
}
