{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn generate && tsc --build",
    "clean": "rimraf dist generated node_modules",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" -c ../.eslintrc.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "generate": "yarn with-env -- prisma generate",
    "migrate": "yarn with-env -- prisma migrate dev"
  },
  "dependencies": {
    "@apollo/federation": "^0.35.4",
    "@apollo/subgraph": "^0.3.3",
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/apollo": "^10.0.7",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/graphql": "^10.0.7",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/terminus": "^8.0.6",
    "@prisma/client": "^3.11.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "graphql": "^16.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.15.0",
    "graphql-type-json": "^0.3.2",
    "lib": "workspace:*",
    "nestjs-prisma": "^0.15.1",
    "prisma-graphql-type-decimal": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "prisma": "^3.11.0",
    "prisma-nestjs-graphql": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  }
}
