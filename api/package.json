{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "dev": "nest start --debug --watch --preserveWatchOutput",
    "start": "node --heapsnapshot-signal=SIGUSR2 dist/src/main.js",
    "start:web": "BACKGROUND_JOBS=false yarn start",
    "start:worker": "BACKGROUND_JOBS=true yarn start",
    "test": "yarn with-env -- jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "tsx --inspect-brk ../node_modules/.bin/jest --runInBand",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "with-env": "npx -y dotenv-cli -e ../.env",
    "generate:schema": "yarn prebuild && SKIP_ENV_CHECK=true nest start --entryFile generate-schema",
    "generate:edgeql": "npx -y concurrently -n edgeql-js,interfaces -c blue,magenta \"npx @edgedb/generate edgeql-js --target ts --force-overwrite\" \"npx @edgedb/generate interfaces\"",
    "generate:ampli": "ampli pull -p ./src/util/ampli --omit-api-keys",
    "generate:gql": "graphql-codegen --config codegen.ts",
    "db:prepare": "yarn db:migrate && yarn db:seed",
    "db:seed": "npx tsx@4.12.1 ./dbschema/seed.ts",
    "db:migrate": "yarn with-env -- edgedb migrate"
  },
  "dependencies": {
    "@amplitude/analytics-node": "^1.3.6",
    "@apollo/server": "^4.10.4",
    "@bull-board/api": "^5.20.1",
    "@bull-board/express": "^5.20.1",
    "@bull-board/nestjs": "^5.20.1",
    "@bull-board/ui": "^5.20.1",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/terminus": "^10.2.3",
    "@pythnetwork/pyth-evm-js": "^1.48.0",
    "@sentry/cli": "^2.32.1",
    "@sentry/node": "^8.7.0",
    "@sentry/profiling-node": "^8.7.0",
    "@songkeys/nestjs-redis": "^10.0.0",
    "abitype": "^1.0.2",
    "bullmq": "^5.7.15",
    "chains": "workspace:*",
    "chardet": "^2.0.0",
    "connect-redis": "^7.1.1",
    "decimal.js": "^10.4.3",
    "deepmerge-ts": "^5.1.0",
    "dotenv": "^16.4.5",
    "edgedb": "^1.5.7",
    "ethers": "^6.13.0",
    "expo-server-sdk": "^3.10.0",
    "express-session": "^1.18.0",
    "graphql": "^16.8.1",
    "graphql-redis-subscriptions": "^2.6.1",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.16.0",
    "ioredis": "^5.3.2",
    "jose": "^5.4.0",
    "lib": "workspace:*",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "neverthrow": "^6.2.2",
    "parse-redis-url-simple": "^1.0.2",
    "redis-semaphore": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-deepmerge": "^7.0.0",
    "ts-pattern": "^5.1.2",
    "viem": "^2.13.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.5.3",
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/luxon": "^3.4.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1"
  }
}
