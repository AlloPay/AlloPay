{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:libs && yarn prisma generate && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:local-services": "docker compose -f ./services.yml up",
    "lint": "eslint \"src/**/*.ts\" -c ../.eslintrc.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "prisma": "yarn with-env -- prisma",
    "generate:gql": "DOTENV_CONFIG_PATH=\"../.env\" graphql-codegen --require dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@golevelup/ts-jest": "^0.3.3",
    "@liaoliaots/nestjs-redis": "^9.0.2",
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/terminus": "^9.1.2",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.4.0",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "async-mutex": "^0.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "workspace:*",
    "connect-redis": "^6.1.3",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "ethers": "^5.7.1",
    "express-session": "^1.17.3",
    "fetch-retry": "^5.0.3",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.18.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.2.3",
    "isomorphic-fetch": "^3.0.0",
    "lib": "workspace:*",
    "luxon": "^3.0.4",
    "nestjs-prisma": "^0.19.0-dev.0",
    "prisma": "^4.4.0",
    "prisma-graphql-type-decimal": "^2.0.4",
    "prisma-nestjs-graphql": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "siwe": "^1.1.6",
    "type-graphql": "^1.1.1",
    "zksync-web3": "^0.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.2",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.0.3",
    "@types/luxon": "^3.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "apollo": "2.34.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
