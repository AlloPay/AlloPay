{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "yarn": "3.x"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:libs && yarn generate && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"src/**/*.ts\" -c ../.eslintrc.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "generate": "yarn with-env -- prisma generate",
    "migrate": "yarn with-env -- prisma migrate dev",
    "migrate:deploy": "yarn with-env -- prisma migrate deploy",
    "migrate:reset": "yarn with-env -- prisma migrate reset",
    "seed": "yarn with-env -- prisma db seed",
    "studio": "yarn with-env -- prisma studio"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@graphql-tools/delegate": "^8.5.4",
    "@graphql-tools/stitch": "^8.5.2",
    "@graphql-tools/utils": "^8.6.3",
    "@graphql-tools/wrap": "^8.4.6",
    "@nestjs/apollo": "^10.0.7",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/graphql": "^10.0.7",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/terminus": "^8.0.6",
    "@paljs/plugins": "^4.0.18",
    "@prisma/client": "^3.13.0",
    "@quixo3/prisma-session-store": "^3.1.3",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "config": "workspace:*",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "ethers": "^5.6.2",
    "express-session": "^1.17.2",
    "graphql": "^16.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.15.0",
    "graphql-type-json": "^0.3.2",
    "lib": "workspace:*",
    "luxon": "^2.3.1",
    "nestjs-prisma": "^0.15.1",
    "prisma": "^3.13.0",
    "prisma-graphql-type-decimal": "^2.0.0",
    "prisma-nestjs-graphql": "^15.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "siwe": "^1.1.6",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  }
}
