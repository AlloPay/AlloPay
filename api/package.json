{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "(cd .. && yarn lib build) && yarn prisma generate",
    "build": "yarn prebuild && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:local-services": "docker compose -f ./services.yml up",
    "lint": "eslint \"src/**/*.ts\" -c ../.eslintrc.js --fix",
    "test": "RELEASE_ENV=\"test\" jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "prisma": "yarn with-env -- prisma",
    "generate:gql": "DOTENV_CONFIG_PATH=\"../.env\" graphql-codegen --require dotenv/config --config codegen.yml",
    "generate:schema": "yarn prebuild && npx node -r tsconfig-paths/register -r ts-node/register ./src/generate-schema.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@golevelup/ts-jest": "^0.3.4",
    "@liaoliaots/nestjs-redis": "^9.0.4",
    "@nestjs/apollo": "^10.1.4",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/graphql": "^10.1.5",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/terminus": "^9.1.2",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^4.6.0",
    "apollo-server-core": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "async-mutex": "^0.4.0",
    "class-transformer": "^0.5.1",
    "connect-redis": "^6.1.3",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "ethers": "^5.7.2",
    "expo-server-sdk": "^3.7.0",
    "express-session": "^1.17.3",
    "fetch-retry": "^5.0.3",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.20.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.2.4",
    "isomorphic-fetch": "^3.0.0",
    "lib": "workspace:*",
    "luxon": "^3.1.0",
    "nestjs-prisma": "^0.19.0",
    "prisma": "^4.6.0",
    "prisma-graphql-type-decimal": "^2.0.4",
    "prisma-nestjs-graphql": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "siwe": "^1.1.6",
    "ts-pattern": "^4.0.5",
    "type-graphql": "^1.1.1",
    "zksync-web3": "^0.11.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/connect-redis": "^0.0.19",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.2.2",
    "@types/luxon": "^3.1.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "apollo": "2.34.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
