{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "(cd .. && yarn lib build) && yarn generate:gql",
    "build": "yarn prebuild && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "clean:services": "docker compose -f ./services.yml down -v",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:production": "yarn db:migrate && node dist/dbschema/seed.js && node dist/src/main.js",
    "start:services": "docker compose -f ./services.yml up",
    "test": "yarn with-env -- jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "npx -y dotenv-cli -e ../.env",
    "generate:schema": "yarn prebuild && SKIP_ENV_CHECK=true nest start --entryFile generate-schema",
    "generate:edgeql": "npx -y concurrently -n edgeql-js,interfaces -c blue,magenta \"npx @edgedb/generate edgeql-js --target ts --force-overwrite\" \"npx @edgedb/generate interfaces\"",
    "generate:gql": "graphql-codegen --config codegen.ts",
    "db:seed": "npx -y ts-node ./dbschema/seed.ts",
    "db:migrate": "yarn with-env -- edgedb migrate"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@bull-board/express": "^5.8.0",
    "@bull-board/nestjs": "^5.8.0",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/terminus": "^10.0.1",
    "@sentry/node": "^7.64.0",
    "@songkeys/nestjs-redis": "^10.0.0",
    "@urql/core": "4.1.1",
    "@urql/exchange-request-policy": "^1.0.2",
    "@urql/exchange-retry": "^1.2.0",
    "abitype": "^0.9.6",
    "async-mutex": "^0.4.0",
    "bull": "^4.11.3",
    "chardet": "^1.6.0",
    "class-transformer": "^0.5.1",
    "connect-redis": "^7.1.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "edgedb": "^1.3.6",
    "ethers": "^5.7.2",
    "expo-server-sdk": "^3.7.0",
    "express-session": "^1.17.3",
    "fetch-retry": "^5.0.6",
    "graphql": "^16.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.22.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.14.0",
    "ioredis": "^5.3.2",
    "jose": "^4.14.6",
    "lib": "workspace:*",
    "luxon": "^3.4.0",
    "nestjs-request-context": "^3.0.0",
    "neverthrow": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "siwe": "^1.1.6",
    "ts-deepmerge": "^6.2.0",
    "ts-pattern": "^5.0.5",
    "viem": "^1.6.0",
    "zksync-web3": "^0.14.3"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.3.3",
    "@golevelup/ts-jest": "^0.4.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@n1ru4l/graphql-codegen-relay-optimizer-plugin": "^5.0.0",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.5.3",
    "@types/luxon": "^3.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "apollo": "^2.34.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "jest-environment-node": "^29.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0"
  }
}
