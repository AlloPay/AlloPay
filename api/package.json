{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "(cd .. && yarn lib build) && yarn prisma generate",
    "build": "yarn prebuild && nest build",
    "clean": "npx rimraf dist generated node_modules",
    "clean:services": "docker compose -f ./services.yml down -v",
    "format": "prettier --config ../.prettierrc --write \"src/**/*.ts\"",
    "start": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:services": "docker compose -f ./services.yml up",
    "test": "yarn with-env -- jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/.bin/jest --runInBand",
    "with-env": "dotenv -e ../.env",
    "prisma": "yarn with-env -- prisma",
    "generate:gql": "DOTENV_CONFIG_PATH=\"../.env\" graphql-codegen --require dotenv/config --config codegen.yml",
    "generate:schema": "yarn prebuild && SKIP_ENV_CHECK=true nest start --entryFile generate-schema"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@golevelup/ts-jest": "^0.3.5",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.2.1",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/terminus": "^9.2.1",
    "@paljs/plugins": "^5.2.0",
    "@prisma/client": "^4.11.0",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "async-mutex": "^0.4.0",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "connect-redis": "^7.0.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "dotenv-vault-core": "^0.7.0",
    "ethers": "^5.7.2",
    "expo-server-sdk": "^3.7.0",
    "express-session": "^1.17.3",
    "fetch-retry": "^5.0.3",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.12.0",
    "ioredis": "5.2.5",
    "lib": "workspace:*",
    "luxon": "^3.3.0",
    "nestjs-request-context": "^2.1.0",
    "prisma": "^4.11.0",
    "prisma-graphql-type-decimal": "^2.0.6",
    "prisma-nestjs-graphql": "^17.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "siwe": "^1.1.6",
    "ts-deepmerge": "^6.0.2",
    "ts-pattern": "^4.2.1",
    "type-graphql": "^1.1.1",
    "zksync-web3": "^0.13.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "9.2.1",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.2.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "apollo": "^2.34.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
