{
  "name": "docs",
  "private": true,
  "scripts": {
    "build": "yarn generate && docusaurus build",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate": "(cd .. && yarn api generate:schema) && yarn generate:docs && yarn generate:gql",
    "generate:docs": "yarn docusaurus graphql-to-doc",
    "generate:gql": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@apollo/explorer": "^2.0.2",
    "@docusaurus/core": "latest",
    "@docusaurus/preset-classic": "latest",
    "@docusaurus/remark-plugin-npm2yarn": "latest",
    "@emotion/react": "^11.10.5",
    "@graphql-markdown/docusaurus": "^1.16.3",
    "@mdx-js/react": "^1.6.22",
    "bufferutil": "^4.0.7",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "dotenv-vault-core": "^0.7.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lib": "workspace:*",
    "plugin-image-zoom": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.7.6",
    "sass": "^1.58.1",
    "use-deep-compare-effect": "^1.8.1",
    "utf-8-validate": "^5.0.10",
    "zksync-web3": "^0.13.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "latest",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/introspection": "^2.2.3",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@tsconfig/docusaurus": "^1.0.6",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
