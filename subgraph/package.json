{
  "name": "subgraph",
  "version": "0.1",
  "scripts": {
    "auth": "graph auth --product hosted-service",
    "configure:testnet": "mustache config/testnet.json subgraph.mustache.yaml > subgraph.yaml",
    "configure:metanet": "mustache config/metanet.json subgraph.mustache.yaml > subgraph.yaml",
    "generate:abi": "(cd .. && yarn workspace contracts build) && node ./scripts/generate-abi.js",
    "generate": "yarn generate:abi && graph codegen && node ./scripts/fix-generated.js",
    "build": "graph build",
    "clean": "npx rimraf build data generated subgraph.yaml",
    "create:testnet": "graph create --node http://graph.metasafe.fi:8020/ hbriese/metasafe",
    "remove:testnet": "graph remove --node http://graph.metasafe.fi:8020/ hbriese/metasafe",
    "deploy:testnet": "graph deploy --node http://graph.metasafe.fi:8020/ --ipfs http://graph.metasafe.fi:5001 hbriese/metasafe",
    "deploy:hosted": "graph deploy --product hosted-service hbriese/metasafe"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.26.0",
    "@graphprotocol/graph-ts": "0.26.0"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
