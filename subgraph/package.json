{
  "name": "subgraph",
  "scripts": {
    "configure:testnet": "mustache networks/testnet.json subgraph.mustache.yaml > subgraph.yaml",
    "generate": "(cd .. && yarn workspace contracts build) && yarn configure:testnet && graph codegen",
    "build:testnet": "yarn configure:testnet && yarn graph build",
    "clean": "npx rimraf build data generated subgraph.yaml",
    "auth": "graph auth --product hosted-service",
    "deploy": "graph deploy --product hosted-service"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.43.0",
    "@graphprotocol/graph-ts": "0.29.3"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}
