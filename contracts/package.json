{
  "name": "contracts",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "prebuild": "(cd .. && yarn lib build)",
    "build": "yarn prebuild && yarn compile",
    "typechain": "hardhat typechain",
    "analyze": "myth analyze src/*",
    "typecheck": "tsc --noEmit",
    "test": "yarn typecheck && CHAIN=local hardhat test",
    "test:debug": "yarn test --logs --trace",
    "deploy:account-impl": "CHAIN=testnet ts-node scripts/deployAccountImpl.ts",
    "deploy:factory": "CHAIN=testnet ts-node scripts/deployProxyFactory.ts",
    "clean": "hardhat clean && npx rimraf artifacts cache"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync-chai-matchers": "^0.1.2",
