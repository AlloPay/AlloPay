{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",

    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@provider": ["./src/features/provider"],
      "@features/*": ["./src/features/*"],
      "@mutations": ["./src/gql/mutations"],
      "@queries": ["./src/gql/queries"],
      "@gql/*": ["./src/gql/*"],
      "@util/*": ["./src/util/*"],
      "~/*": ["./src/*"]
    }
  }
}
