{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "postinstall": "expo-yarn-workspaces postinstall",
    "publish": "expo publish --non-interactive",
    "build:web": "yarn build:libs && expo build:web",
    "start": "expo start",
    "start:android": "yarn start --android",
    "test": "jest",
    "generate": "yarn clean && concurrently --names api,sub,uni -c yellow,blue \"yarn generate:api\" \"yarn generate:sg\" \"yarn generate:uniswap\"",
    "generate:api": "apollo client:codegen ./src/gql/api.generated.ts --config apollo.api.config.js --target typescript --outputFlat",
    "generate:sg": "apollo client:codegen ./src/gql/subgraph.generated.ts --config apollo.sg.config.js --target typescript --outputFlat",
    "generate:uniswap": "apollo client:codegen ./src/gql/uniswap.generated.ts --config apollo.uniswap.config.js --target typescript --outputFlat",
    "generate:erc20": "npx abi-types-generator ./src/features/token/erc20/erc20.abi.json --provider ethers_v5 --name erc20 --output ./src/features/token/erc20",
    "clean": "rimraf web-build"
  },
  "expo-yarn-workspaces": {
    "//": "https://www.npmjs.com/package/expo-yarn-workspaces"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@ethersproject/shims": "^5.6.0",
    "@expo/config": "^6.0.19",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "apollo3-cache-persist": "^0.14.0",
    "babel-plugin-formatjs": "^10.3.19",
    "color": "^4.2.3",
    "config": "workspace:*",
    "ethers": "^5.6.1",
    "expo": "~44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-localization": "~12.0.0",
    "expo-secure-store": "~11.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "expo-yarn-workspaces": "^1.7.0",
    "intl": "^1.2.5",
    "lib": "workspace:*",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "^5.24.8",
    "react-native": "0.64.3",
    "react-native-get-random-values": "^1.7.2",
    "react-native-paper": "^4.12.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "use-async-effect": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/color": "^3.0.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components-react-native": "^5.1.3",
    "@types/styled-system": "^5.1.15",
    "apollo": "^2.33.10",
    "concurrently": "^7.1.0",
    "eslint-plugin-formatjs": "^3.1.0",
    "ethereum-abi-types-generator": "^1.3.2",
    "jest": "^27.5.1",
    "jest-expo": "~44.0.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  }
}
