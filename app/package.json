{
  "name": "app",
  "private": true,
  "version": "0.1.0",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "postinstall": "expo-yarn-workspaces postinstall",
    "publish": "expo publish --non-interactive",
    "build": "(cd .. && yarn lib build && yarn contracts build) && yarn generate",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "generate": "yarn clean && yarn generate:api && yarn generate:sg",
    "generate:api": "apollo client:codegen --config apollo.api.config.js --target typescript --globalTypesFile ./src/gql/__generated__/apiGlobalTypes.ts",
    "generate:sg": "apollo client:codegen --config apollo.sg.config.js --target typescript --globalTypesFile ./src/gql/__generated__/sgGlobalTypes.ts",
    "clean": "rimraf ./src/gql/__generated__"
  },
  "expo-yarn-workspaces": {
    "//": "https://www.npmjs.com/package/expo-yarn-workspaces"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@ethersproject/shims": "^5.6.0",
    "@expo/config": "^6.0.19",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "config": "workspace:*",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.1",
    "expo": "~44.0.0",
    "expo-secure-store": "~11.1.0",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "expo-yarn-workspaces": "^1.7.0",
    "lib": "workspace:*",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-get-random-values": "^1.7.2",
    "react-native-web": "0.17.1",
    "use-async-effect": "^2.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "apollo": "^2.33.10",
    "concurrently": "^7.1.0",
    "jest": "^27.5.1",
    "jest-expo": "~44.0.1",
    "react-test-renderer": "17.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  }
}
