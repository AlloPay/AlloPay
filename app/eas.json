{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "env": {
        "RELEASE_ENV": "production",
        "CHAIN": "testnet",
        "API_URL": "https://api.allopay.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/allopay3",
        "ACCOUNT_IMPL_TESTNET": "0x98df98fA082f05796349C17DF75447998F651266",
        "PROXY_FACTORY_TESTNET": "0xf74f2b05c68c3605606816b557565514E546934f",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "development": {
      "extends": "base",
      "channel": "dev",
      "env": {
        "RELEASE_ENV": "development",
        "CHAIN": "testnet",
        "API_URL": "https://api-dev.allopay.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/allopay-dev3",
        "ACCOUNT_IMPL_TESTNET": "0x98df98fA082f05796349C17DF75447998F651266",
        "PROXY_FACTORY_TESTNET": "0xf74f2b05c68c3605606816b557565514E546934f",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "production:alpha": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    },
    "development": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
