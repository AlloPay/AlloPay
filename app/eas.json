{
  "cli": {
    "version": ">= 3.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "main": {
      "extends": "base",
      "channel": "main",
      "env": {
        "API_URL": "https://api.zallo.io",
        "API_GQL_WS": "wss://api.zallo.io/graphql",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo-2",
        "ACCOUNT_IMPL_TESTNET": "0x53b0AC596b0c2ED4dD8442F7196e1Db4e28a2816",
        "PROXY_FACTORY_TESTNET": "0x5687f4FFa1104a1F0d13Ef969598709254A72f6f"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "base": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "releaseStatus": "completed"
      }
    },
    "beta": {
      "extends": "base",
      "android": {
        "track": "beta"
      }
    },
    "alpha": {
      "android": {
        "track": "alpha"
      }
    },
    "development": {
      "extends": "base",
      "android": {
        "track": "internal"
      }
    }
  }
}
