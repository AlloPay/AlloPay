{
  "cli": {
    "version": ">= 3.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "main": {
      "extends": "base",
      "channel": "main",
      "env": {
        "API_URL": "https://api.zallo.io",
        "API_GQL_WS": "wss://api.zallo.io/graphql",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo-2",
        "ACCOUNT_IMPL_TESTNET": "0x254dd774d6aeD5846fE34cdc014D4CAC23b24291",
        "PROXY_FACTORY_TESTNET": "0x7E2B7a3a7a980159cE92974267D1c49B2f3B448f",
        "MULTI_CALL_TESTNET": "0xfF96bad2582Cd62d52214A401ccD0BD35D6CB351"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "alpha": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    },
    "development": {
      "internal": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
