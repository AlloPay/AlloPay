{
  "cli": {
    "version": ">= 3.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "env": {
        "RELEASE_ENV": "production",
        "CHAIN": "testnet",
        "API_URL": "https://api.zallo.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo",
        "ACCOUNT_IMPL_TESTNET": "0x53b0AC596b0c2ED4dD8442F7196e1Db4e28a2816",
        "PROXY_FACTORY_TESTNET": "0x5687f4FFa1104a1F0d13Ef969598709254A72f6f",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "development": {
      "extends": "base",
      "channel": "dev",
      "env": {
        "RELEASE_ENV": "development",
        "CHAIN": "testnet",
        "API_URL": "https://dev.api.zallo.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo-dev",
        "ACCOUNT_IMPL_TESTNET": "0x53b0AC596b0c2ED4dD8442F7196e1Db4e28a2816",
        "PROXY_FACTORY_TESTNET": "0x5687f4FFa1104a1F0d13Ef969598709254A72f6f",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "production:alpha": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    },
    "development": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
