{
  "cli": {
    "version": ">=3.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "main": {
      "extends": "base",
      "channel": "main",
      "env": {
        "API_URL": "https://api.zallo.io",
        "API_GQL_WS": "wss://api.zallo.io/graphql",
        "ACCOUNT_IMPL_TESTNET": "0x90CfF0205437396b0C6b0d575BF3512d1175A6c3",
        "PROXY_FACTORY_TESTNET": "0x1E655C43C74d9559EFEC0F6f501A89AB350AF122"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "base": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "releaseStatus": "completed"
      }
    },
    "beta": {
      "extends": "base",
      "android": {
        "track": "beta"
      }
    },
    "alpha": {
      "android": {
        "track": "alpha"
      }
    },
    "development": {
      "extends": "base",
      "android": {
        "track": "internal"
      }
    }
  }
}
