{
  "cli": {
    "version": ">= 3.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "env": {
        "API_URL": "https://api.zallo.io",
        "API_GQL_WS": "wss://api.zallo.io/graphql",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo",
        "ACCOUNT_IMPL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626",
        "PROXY_FACTORY_TESTNET": "0xee98b0EC2279a37fd2142178b6aC6E4237F0e685",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "development": {
      "extends": "base",
      "channel": "dev",
      "env": {
        "API_URL": "https://dev.api.zallo.io",
        "API_GQL_WS": "wss://dev.api.zallo.io/graphql",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/zallo-dev",
        "ACCOUNT_IMPL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626",
        "PROXY_FACTORY_TESTNET": "0xee98b0EC2279a37fd2142178b6aC6E4237F0e685",
        "MULTI_CALL_TESTNET": "0x164921b76384661089a6295b57F7873784BaB626"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "RELEASE_ENV": "production"
      }
    }
  },
  "submit": {
    "production:alpha": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    },
    "development": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
