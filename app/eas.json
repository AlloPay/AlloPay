{
  "cli": {
    "version": ">= 0.59.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "16.14.2",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js",
        "CHAIN": "testnet",
        "ACCOUNT_IMPL_TESTNET": "0x45a35E86D42B6B23f95Ba3499Ac1F4D94e5AaB2a",
        "PROXY_FACTORY_TESTNET": "0x8d5D8bE272EDF1A14242AD0157E1C5909567185f",
        "MULTI_CALL_TESTNET": "0x00dF06A3f538eCcB90FEC78a2CAeF84e300f5183"
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "preview": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal"
    },
    "development": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
