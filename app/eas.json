{
  "cli": {
    "version": ">= 0.59.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "16.14.2",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js",
        "CHAIN": "testnet"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "preview": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal"
    },
    "development": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal"
      }
    }
  }
}
