{
  "cli": {
    "version": ">=3.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.12.1",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "main": {
      "extends": "base",
      "channel": "main",
      "env": {
        "API_URL": "https://api.zallo.io",
        "API_GQL_WS": "wss://api.zallo.io/graphql"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "base": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "releaseStatus": "completed"
      },
      "ios": {
        "ascAppId": "6444339269",
        "sku": "EX1668082482987"
      }
    },
    "beta": {
      "extends": "base",
      "android": {
        "track": "beta"
      }
    },
    "alpha": {
      "android": {
        "track": "alpha"
      }
    },
    "development": {
      "extends": "base",
      "android": {
        "track": "internal"
      }
    }
  }
}
