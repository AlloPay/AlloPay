{
  "cli": {
    "version": ">= 0.59.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "16.14.2",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js",
        "CHAIN": "testnet",
        "ACCOUNT_IMPL_TESTNET": "0x0DE63Ab281e4241D38e2C0CB2a55661912F6422d",
        "PROXY_FACTORY_TESTNET": "0x3FE53d6dCC73bAadEE100ECAe4dB04066C03dD34",
        "MULTI_CALL_TESTNET": "0xBF6f1B40Bf1CcA67724A9d4992495eE81E91ABE5"
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "preview": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal"
    },
    "development": {
      "extends": "base",
      "channel": "main",
      "distribution": "internal",
      "developmentClient": true,
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
