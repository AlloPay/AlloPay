{
  "cli": {
    "version": ">= 2.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "16.14.2",
      "env": {
        "ENTRY_FILE": "./__generated__/AppEntry.js"
      },
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "production": {
      "extends": "base",
      "channel": "main",
      "env": {
        "RELEASE_ENV": "production",
        "CHAIN": "testnet",
        "API_URL": "https://api.allopay.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/allopay",
        "ACCOUNT_IMPL_TESTNET": "0x4f9a8a04F7480c1FDeA283Ce35A71D1790361363",
        "PROXY_FACTORY_TESTNET": "0x2e5018e81E653d218396E7C151B5a84dEfaa9A67",
        "MULTI_CALL_TESTNET": "0x51D4c48FFA8c224ca4ed1cf43E53Fc7b90777433"
      }
    },
    "development": {
      "extends": "base",
      "channel": "dev",
      "env": {
        "RELEASE_ENV": "development",
        "CHAIN": "testnet",
        "API_URL": "https://api-dev.allopay.io",
        "SUBGRAPH_GQL_URL": "https://api.thegraph.com/subgraphs/name/hbriese/allopay-dev",
        "ACCOUNT_IMPL_TESTNET": "0x4f9a8a04F7480c1FDeA283Ce35A71D1790361363",
        "PROXY_FACTORY_TESTNET": "0x2e5018e81E653d218396E7C151B5a84dEfaa9A67",
        "MULTI_CALL_TESTNET": "0x51D4c48FFA8c224ca4ed1cf43E53Fc7b90777433"
      }
    },
    "dev-client": {
      "extends": "base",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "production:alpha": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "alpha",
        "releaseStatus": "completed"
      }
    },
    "development": {
      "android": {
        "serviceAccountKeyPath": "./play-service-account.secret.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
